<div class="header">
    <h1>Title</h1>
    <h1>Listen</h1>
</div>
<div class="main-tracks">
    {{#each items}}
    <div class="tracks">
        <h4>{{this.name}}</h4>
        <figure>
            {{!-- <figcaption>Listen {{this.name}}:</figcaption> --}}
            <audio controls src={{this.preview_url}}>
                Your browser does not support the
                <code>audio</code> element.
            </audio>
        </figure>
    </div>
    {{/each}}
</div>